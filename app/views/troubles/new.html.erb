<h1>Troubles</h1>

<%= form_for @trouble do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>
  <br><br>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <br><br>

  <%= f.label :date %>
  <%= f.date_field :date %>
  <br><br>

  <%= f.label :location %>
  <%= f.text_field :location %>
  <br><br>

 <%= f.label :description %>
  <%= f.text_field :description %>
  <br><br>

<!-- 
  <%= f.label :images %>
  <%= f.file_field :images %>
  <br><br> -->

  <%= f.submit 'Report Trouble' %>

<% end %>